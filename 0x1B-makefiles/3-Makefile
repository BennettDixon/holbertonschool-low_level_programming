CC=gcc
SRC = holberton.c main.c
OBJ = holberton.o main.o
NAME=holberton
RM=rm

all : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

.PHONY : clean
clean :
	-$(RM) -f *~ $(NAME)

.PHONY : oclean
oclean :
	-$(RM) -f $(OBJ)

.PHONY : fclean
fclean :
	-$(RM) -f ~* $(NAME) $(OBJ)

re :
	$(CC) -c $(SRC) 

$(OBJ) : m.h
